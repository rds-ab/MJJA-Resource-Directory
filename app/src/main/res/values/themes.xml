<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.JuvenileJustice" parent="Theme.MaterialComponents.DayNight.NoActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">#012202</item>
        <item name="colorPrimaryVariant">#012303</item>
        <item name="colorOnPrimary">#151515</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">#ffffff</item>
        <item name="colorSecondaryVariant">#A88007</item>
        <item name="colorOnSecondary">#151515</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor" tools:targetApi="l">?attr/colorPrimaryVariant</item>
        <!-- Customize your theme here. -->
    </style>

    <style name="Theme.JuvenileJustice.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>

    <style name="Theme.JuvenileJustice.NoActionBar.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>

    <style name="Theme.JuvenileJustice.NoActionBar.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />

    <style name="Theme.JuvenileJustice.NoActionBar.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />
</resources>